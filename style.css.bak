/*в правилі для body нічого змінювати НЕ потрібно*/
body{
	margin:20px auto;
	padding:0;
	width: 70%;
	min-height: 500px;
	box-shadow: 2px 2px 5px grey;
	font-family: Arial;
	
}

/*в цьому правилі нічого не потрібно змінювати*/
.slogan{
	font-size: 20px;
	text-align: center;
}

/*в цьому правилі нічого не потрібно змінювати*/
.footer{
	background:#0e2b3c;
	height: 25px;
	color:white;
	text-align: center;
	font-size: 0.5em;
}


/*тут бажано додати одну властивість,  
яка змінює режим розрахунку загальних розмірів*/
.header{
	width: 100%;
	height: 50px;
	background: #a6dbfb;
	padding: 5px;
	color:#0e2b3c;
	
}

/*зображенням потрібно додати по одній властивості*/
.oreo-logo{
	height: 40px;
	
}

.oreo-cookie{
	height: 40px;
	
}


.main{
	min-height: 425px;
	background: #deebf4;
	
}
table{
border: 2px solid grey;
border-collapse: collapse;
}
td,th{
border: 2px solid grey;
height: 25px;
width: 200px;
padding: 5px;
text-align: center;
}
.f1{
	background-color:yellow;
}
.f2{
	background-color:orange;
}




